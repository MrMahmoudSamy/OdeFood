@page
@model OdeFood.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>Restaurants</h1>



<table class="table">
    <thead>
        <tr>
            <th scope="col">Restaurant</th>
            <th scope="col">Location</th>
            <th scope="col">Cuisine</th>
        </tr>
    </thead>
    <tbody>

        @foreach(var rest in Model.restaurant)
                {
                       <tr>
                         <td>@rest.Name</td>
                         <td>@rest.Location</td>
                         <td>@rest.Cuisine</td>
                     </tr>
                }


    </tbody>



</table>